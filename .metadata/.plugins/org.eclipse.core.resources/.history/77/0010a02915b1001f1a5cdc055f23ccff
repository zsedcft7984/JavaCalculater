package calculator;

import java.awt.*;
import javax.swing.*;


public class MainFrame extends JFrame {
	public MainFrame() {
		setSize(300,200);
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setTitle("계산기");
		
		setLayout(new FlowLayout());
		JButton button = new  JButton("기본 버튼");
		
		// 컴포넌트 생성 및 추가
		this.add(button);
		setVisible(true);
	}

}
